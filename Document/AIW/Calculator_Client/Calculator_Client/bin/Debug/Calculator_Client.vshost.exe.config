<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="CalculatorSoapBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8181/Webservice/services/Calculator"
                binding="basicHttpBinding" bindingConfiguration="CalculatorSoapBinding"
                contract="calculator.Calculator" name="Calculator" />
        </client>
    </system.serviceModel>
</configuration>